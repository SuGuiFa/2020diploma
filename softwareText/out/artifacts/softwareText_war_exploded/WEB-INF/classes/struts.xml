<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<constant name="struts.devMode" value="true"/>
	<constant name="struts.configuration.xml.reload" value="true"/>
	<constant name="struts.multipart.saveDir" value="/tmp"/>
	<constant name="struts.i18n.encoding" value="UTF-8"/>
	<constant name="struts.enable.DynamicMethodInvocation" value="true"/>

	<constant name="struts.i18n.reload" value="true"/>
	<constant name="struts.ognl.allowStaticMethodAccess" value="true"/>
	<constant name="struts.action.extension" value="action"/>
	<constant name="struts.multipart.maxSize" value="1073741824" />
	<constant name="struts.ui.theme" value="simple"/>
	<constant name="struts.custom.i18n.resources" value="applicationResource"/>

	<package name="com.textUser.action" extends="struts-default" namespace="/">	
	<action name="ShowViewAction" class="com.textUser.action.ShowViewAction">
		<result name="txtboundarymethod">/front/txtboundarymethod.jsp</result>
		<result name="comboboundarymethod">/front/comboboundarymethod.jsp</result>		
		<result name="combinedmethod">/front/combinedmethod.jsp</result>
		<result name="txtequivalentmethod">/front/txtequivalentmethod.jsp</result>	
		<result name="thirdcombinedmethod">/front/thirdcombinedmethod.jsp</result>
		<result name="doublecombinedmethod">/front/doublecombinedmethod.jsp</result>
		<result name="usefulcase">/front/usefulcase.jsp</result>
		<result name="comboequivalentmethod">/front/comboequivalentmethod.jsp</result>	
	</action>
	<action name="EquivalentMethodAction" class="com.textUser.action.EquivalentMethodAction">
	</action>
	<action name="CombinedMethodAction" class="com.textUser.action.CombinedMethodAction">
	</action>
	<action name="BoundaryMethodAction" class="com.textUser.action.BoundaryMethodAction">
	</action>
	<action name="ComboBoundaryMethodAction" class="com.textUser.action.ComboBoundaryMethodAction">
	</action>
	<action name="DoubleCombinedMethodAction" class="com.textUser.action.DoubleCombinedMethodAction">
	</action>
	<action name="ThirdCombinedMethodAction" class="com.textUser.action.ThirdCombinedMethodAction">
	</action>
	<action name="UsefulCaseAction" class="com.textUser.action.UsefulCaseAction">
	</action>
	<action name="ComboEquivalentMethodAction" class="com.textUser.action.ComboEquivalentMEthodAction">
	</action>
	
	</package>



</struts>    
